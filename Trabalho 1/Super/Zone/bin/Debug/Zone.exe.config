<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="nextPort" value ="4321"/>
      <add key="port" value="1234" />  
    </appSettings>
  <system.runtime.remoting>
    <application>
      <channels>
        <channel ref="http" port="1234">
          <clientProviders>
            <formatter ref="binary"/>
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </serverProviders>
        </channel>
      </channels>
      <client>
        <!-- Namespace.Type, Assembly  -->
        <wellknown type="ISuperInterfaces.IStockManager, ISuper" url="http://localhost:1234/stockmanager"/>
        <wellknown type="ISuperInterfaces.IZone, ISuper" url="http://localhost:1234/zone"/>
      </client>
    </application>
  </system.runtime.remoting>
</configuration>