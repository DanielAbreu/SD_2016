<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="url" value="http://localhost:{0}" />
      <add key="StockPath" value="Stock1.xml" />  
    </appSettings>
    <system.runtime.remoting>
      <application>
        <channels>
          <channel ref="http">
            <clientProviders>
              <formatter ref="soap"/>
            </clientProviders>
            <serverProviders>
              <formatter ref="soap" typeFilterLevel="Full"/>
            </serverProviders>
          </channel>
        </channels>
        <!--<client>
          -->
        <!-- Namespace.Type, Assembly  -->
        <!--
          <wellknown type="ISuperInterfaces.IStockManager, ISuper" url="http://localhost:1234/stockmanager"/>
          <wellknown type="ISuperInterfaces.IZone, ISuper" url="http://localhost:1234/zone"/>
        </client>-->
      </application>
    </system.runtime.remoting>
</configuration>